{
  "name": "vscode-react-webview-starter",
  "displayName": "vscode-react-webview-starter",
  "description": "",
  "version": "0.0.1",
  "packageManager": "npm@10.8.2",
  "repository": {
    "url": "https://github.com/estruyf/vscode-react-webview-template",
    "type": "git"
  },
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "otterDr.otterView",
          "name": "OtterDr",
          "icon": ".assets/webview.png"
        }
      ]
    },
    "commands": [
      {
        "command": "otterDr.openWebview",
        "title": "Explorer: Open OtterDr View"
      },
      {
        "command": "otterDr.setApiKey",
        "title": "OtterDr: Update API Key"
      },
      {
        "command": "otterDr.deleteApiKey",
        "title": "OtterDr: Delete API Key"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
"compile": "webpack --mode production --config ./webpack/extension.config.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:ext": "webpack --mode development --config ./webpack/extension.config.js",
    "watch:wv": "webpack serve --mode development --config ./webpack/webview.config.js",
    "package": "npm run clean && npm-run-all --parallel package:*",
    "package:ext": "webpack --mode production --config ./webpack/extension.config.js",
    "package:wv": "webpack --mode production --config ./webpack/webview.config.js",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "clean": "rimraf dist",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@estruyf/vscode": "1.1.0",
    "@types/mocha": "10.0.6",
    "@types/node": "18.19.47",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "@types/uuid": "8.3.4",
    "@types/vscode": "1.71.0",
    "@types/webpack": "5.28.1",
    "@types/webpack-dev-server": "4.7.2",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vscode/test-electron": "2.1.5",
    "css-loader": "6.7.1",
    "eslint": "8.57.1",
    "glob": "10.3.10",
    "mocha": "10.4.0",
    "npm-run-all": "4.1.5",
    "postcss-loader": "7.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "rimraf": "5.0.5",
    "style-loader": "3.3.1",
    "ts-loader": "9.4.1",
    "typescript": "4.9.5",
    "webpack": "5.97.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.2.0",
    "webpack-manifest-plugin": "5.0.0"
  },
  "dependencies": {
    "html-entities": "2.6.0",
    "openai": "6.17.0",
    "vscrui": "0.3.0"
  }
}
